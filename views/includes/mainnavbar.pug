div.navbar.navbar-default
    div.navbar-header
        a.navbar-brand(href="/")= title
    ul.nav.navbar-nav
        each item in mainnavigation
            if item.submenu
                li.dropdown
                    +subitem(item,true)
            else
                li(class=(item.active ? 'active' : ''))
                    a(href=item["link"])= t(item["name"])